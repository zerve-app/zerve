{
  "name": "zoo-server-standalone",
  "private": true,
  "version": "0.0.0",
  "types": "index.ts",
  "main": "index.ts",
  "scripts": {
    "start": "node ./build/ZooServerStandalone.js",
    "start:dev": "cross-env NODE_ENV=dev nodemon ./build/ZooServerStandalone.js",
    "build": "esbuild ./ZooServerStandalone.ts --bundle --platform=node --outfile=build/ZooServerStandalone.js",
    "build:dev": "yarn build --watch",
    "dev": "concurrently \"yarn start:dev\" \"yarn build:dev\"",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "yarn lint:fix"
  },
  "dependencies": {
    "@zerve/auth": "*",
    "@zerve/core": "*",
    "@zerve/data": "*",
    "@zerve/ledger": "*",
    "@zerve/message-email-sendgrid": "*",
    "@zerve/message-sms-twilio": "*",
    "@zerve/node": "*",
    "@zerve/store": "*",
    "@zerve/system-commands": "*",
    "@zerve/system-fetch": "*",
    "@zerve/system-files": "*",
    "@zerve/system-ssh": "*",
    "ajv": "^8.10.0",
    "body-parser": "^1.19.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "next-compose-plugins": "^2.2.1",
    "react-native-web": "^0.17.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.39",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.6.4"
  }
}
